    // error_message.value = \"\";\n            store.dispatch(\"login\", {\n                username: username.value,\n                password: password.value,\n                success(resp) {\n                    store.dispatch(\"getinfo\", {\n                        success() {\n                            router.push({ name: 'home' });\n                            console.log(store.state.user);\n                        }\n                    })\n                },\n                error(resp) {\n                    error_message.value = \"用户名或密码错误\";\n                }\n            })\n        }\n\n        return {\n            username,\n            password,\n            error_message,\n            login,\n        }\n    }\n}\n</script>\n\n<style scoped>\nbutton {\n    width: 100%;\n}\ndiv.error-message {\n    color: red;\n}\n</style>"]}]}